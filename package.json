{
  "name": "postcss-clean-initial-values",
  "version": "2.0.0",
  "description": "PostCSS plugin for removing rules setting the initial value",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "initial value"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src/*.ts",
    "ci-test": "npm run lint && npm run test",
    "build": "tsc"
  },
  "author": "Albin Jacobsson <albin.jacobsson@gmail.com>",
  "license": "MIT",
  "repository": "Sertion/postcss-clean-initial-values",
  "bugs": {
    "url": "https://github.com/Sertion/postcss-clean-initial-values/issues"
  },
  "homepage": "https://github.com/Sertion/postcss-clean-initial-values",
  "dependencies": {
    "mdn-data": "^2.0.2"
  },
  "peerDependencies": {
    "mdn-data": "2.x",
    "postcss": "^8.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@types/css-tree": "^1.0.3",
    "@types/jest": "^26.0.13",
    "@types/node": "^10.14.6",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-jest": "^26.3.0",
    "css-tree": "^1.0.0-alpha.39",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "postcss": "^8.2.10",
    "typescript": "^4.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "jest": {
    "verbose": true,
    "rootDir": "src"
  }
}
